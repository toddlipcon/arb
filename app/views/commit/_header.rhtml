<% commit = header %>
 <table class="diff_header">
  <tr>
    <td class="header">SHA1:</td>
    <td><%=h commit.sha1 %></td>
  </tr>
  <tr>
    <td class="header">Author:</td>
    <td><%=h commit.author %></td>
  </tr>
  <tr>
    <td class="header">Committer:</td>
    <td><%=h commit.committer %></td>
  </tr>
  <tr>
    <td class="header">Log message:</td>
    <td><pre><%=h commit.log_message %></pre></td>
  </tr>
</table>
